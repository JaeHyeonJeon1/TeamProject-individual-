<mapper namespace="com.cafe24.iumium.personnel.statistics.dao.DepartmentMapper">
	
	<!-- 학과 검색 -->
	<select id="searchDepartmentList" resultType="com.cafe24.iumium.common.dto.TeamCode">
	 	SELECT team_code AS teamCode
			,team_name_korean AS teamNameKorean
		FROM haksa_team
		WHERE team_is_used = 유
