<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cafe24.iumium.personnel.statistics.Dao.DepartmentMapper">

	<!-- 교직원검색 리스트 -->
    <select id="searchDepartmentList"  resultType="com.cafe24.iumium.Dto.TeamCode">
		SELECT team_code
			,team_name_korean
		FROM haksa_team
		WHERE team_name_korean like CONCAT('%',학과)
		ORDER BY team_name_korean ASC
    </select>
    
    
</mapper>